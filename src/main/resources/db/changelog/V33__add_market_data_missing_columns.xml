<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="33-add-market-data-missing-columns" author="treishvaam">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="market_data"/>
            <not><columnExists tableName="market_data" columnName="name"/></not>
        </preConditions>
        
        <addColumn tableName="market_data">
            <column name="name" type="VARCHAR(255)"/>
            <column name="volume" type="BIGINT"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>