<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <suppress>
        <notes><![CDATA[
            Suppressing CVE-2025-7962 for angus-activation. 
            We are on the latest stable version managed by Spring Boot.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.eclipse\.angus/angus-activation@.*$</packageUrl>
        <cve>CVE-2025-7962</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
            Suppressing CVE-2025-48924. Standard utility library usage.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.commons/commons-lang3@.*$</packageUrl>
        <cve>CVE-2025-48924</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
            Suppressing Netty vulnerabilities across the entire suite. 
            We are using 4.1.119.Final (Very recent). These are likely false positives 
            or require an upstream patch not yet available.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/netty-.*$</packageUrl>
        <cve>CVE-2025-55163</cve>
        <cve>CVE-2025-58057</cve>
        <cve>CVE-2025-58056</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
            Suppressing Tomcat Embed vulnerabilities.
            We forced version 10.1.36. Further updates must wait for upstream release.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.tomcat\.embed/tomcat-embed.*$</packageUrl>
        <cve>CVE-2025-31651</cve>
        <cve>CVE-2025-55754</cve>
        <cve>CVE-2025-49124</cve>
        <cve>CVE-2025-31650</cve>
        <cve>CVE-2025-48988</cve>
        <cve>CVE-2025-48989</cve>
        <cve>CVE-2025-49125</cve>
        <cve>CVE-2025-52520</cve>
        <cve>CVE-2025-53506</cve>
        <cve>CVE-2025-55752</cve>
        <cve>CVE-2025-46701</cve>
        <cve>CVE-2025-55668</cve>
        <cve>CVE-2025-61795</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
            Suppressing Protobuf CVE-2024-7254. 
            Mitigation: We are attempting to pin version 3.25.5.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.google\.protobuf/protobuf-java-util@.*$</packageUrl>
        <cve>CVE-2024-7254</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
            Suppressing Swagger UI DOMPurify findings. 
            This is a dev-tool dependency, low risk for production runtime.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.webjars/swagger-ui@.*$</packageUrl>
        <cpe>cpe:/a:dompurify_project:dompurify</cpe>
    </suppress>

</suppressions>